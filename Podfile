  pod 'Firebase/Storage'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Email'
  pod 'Firebase/DynamicLinks'
  pod 'Firebase/Messaging'
  pod 'PXSiriWave'
  pod 'MBProgressHUD'
  
  # UI, CollectionView
  pod "UPCarouselFlowLayout"
end
